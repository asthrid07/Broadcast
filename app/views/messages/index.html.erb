<p id="notice"><%= notice %></p>

<h1>Messages</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Message sid</th>
      <th>Body</th>
      <th>Status</th>
      <th>To number</th>
      <th>To zip</th>
      <th>To country</th>
      <th>From number</th>
      <th>From zip</th>
      <th>From country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @messages.each do |message| %>
      <tr>
        <td><%= message.id %></td>
        <td><%= message.message_sid %></td>
        <td><%= message.body %></td>
        <td><%= message.status %></td>
        <td><%= message.to_number %></td>
        <td><%= message.to_zip %></td>
        <td><%= message.to_country %></td>
        <td><%= message.from_number %></td>
        <td><%= message.from_zip %></td>
        <td><%= message.from_country %></td>
        <td><%= link_to 'Show', message %></td>
        <td><%= link_to 'Edit', edit_message_path(message) %></td>
        <td><%= link_to 'Destroy', message, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Message', new_message_path %>
